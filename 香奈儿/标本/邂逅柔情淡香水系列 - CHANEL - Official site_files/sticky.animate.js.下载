!function(){"use strict";define(["jquery","lodash","cssmap","plugins.tweenmax"],function(t,s,i,c){function a(t,s,i){TweenMax.to(t,s,i)}function y(){n.moodboard=t(i(".","moodboard")),n.header=t(i(".","baseNavigation")),n.sticky=t(i(".","stickyContainer")),n.stickySearch=t(i(".","productsNavigation")),n.stickyPdp=t(i(".","stickyProductDetail")),n.stickyElements=t(i(".","baseNavigation"," , .","stickyContainer"," , .","productsNavigation")),n.DOC.one("fnb.sticky.on",function(){n.moodboard.length&&s.delay(function(){a(n.header,.2,{y:0}),a(n.sticky,.2,{y:0})})}),n.DOC.on("fnb.sticky.on",function(){s.delay(function(){if(!t(i(".","stickyOnTop")).length){var s="";n.BODY.hasClass(i("zoneComEnabled"))&&(s="-100px"),n.moodboard.length||n.stickySearch.length?n.BODY.hasClass(i("stickyFixed"))&&(a(n.header,.3,{y:"-120px"}),a(n.sticky,.3,{y:s?s:"-60px"}),a(n.stickySearch,.3,{y:s?s:"-60px",onStart:function(){n.stickySearch.addClass(i("stickyDisplayed"))}})):(a(n.header,.3,{y:"-120px"}),a(n.sticky,.3,{y:s?s:"-60px",onStart:function(){n.sticky.removeClass(i("stickyDisplayed"))}}))}})}).on("fnb.sticky.off",function(){n.sticky.hasClass(i("stickyCheckout"))&&n.sticky.removeClass(i("stickyDisplayed")),n.sticky.hasClass(i("stickyDisplayed"))||a(n.header,.2,{y:0}),(n.moodboard.length||n.stickySearch.length)&&(a(n.header,.2,{y:0}),a(n.sticky,.2,{y:0}),a(n.stickySearch,.2,{y:0})),!n.stickyPdp.length&&!t(i(".","checkoutPage")).length||n.sticky.hasClass(i("stickyDisplayed"))||a(n.sticky,.3,{y:"-120px"}),t(i(".","stickyProductDetail"," , .","stickyCheckoutWrp")).hasClass(i("stickyCheckout"))||n.sticky.hasClass(i("stickyDisplayed"))||a(n.sticky,.2,{y:0,onStart:function(){n.sticky.addClass(i("stickyDisplayed"))}})}).on("fnb.sticky.is.top",function(){a(n.header,.2,{y:0}),a(n.sticky,.2,{y:0}),a(n.stickySearch,.2,{y:0})})}function e(){y()}var n={DOC:t(document),BODY:t("body")};return{init:e}})}();