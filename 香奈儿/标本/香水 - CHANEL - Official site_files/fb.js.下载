CustomSelect.prototype={init:function(){var self=this;self.buildDOM();self.uiManager();},buildDOM:function(){var self=this,$select=self.$select,$selectedOption=this.$select.find('option:selected');if(!$select.parent('.'+self.wrapperClass).length){$select.wrap('<div class="'+self.wrapperClass+'" />');}
self.$wrapper=$select.parent('.'+self.wrapperClass);var $wrapper=self.$wrapper;self.$skinnedSelect=$('<div class="'+self.selectClass+'" data-value="'+$selectedOption.val()+'">'+$selectedOption.text()+'</div>');self.$skinnedSelect.appendTo($wrapper);this.$skinnedList=$('<div class="'+self.listClass+'"></div>');if(this.options.relativeSkinnedLists){this.$skinnedList.appendTo('.'+this.wrapperClass+':last');}else if(this.options.listContainerSelector){this.$skinnedList.appendTo(this.options.listContainerSelector);}else{this.$skinnedList.appendTo('body');}
