var _dataLayerHandler={init:function(){global.dataLayer=global.dataLayer||[];this.listener();},listener:function(){$(document).on({'navItemClick.ccfooter':function(e,item){var id=item.id.replace('x-','');dataLayer.push({'eventcategory':'footer'+_division,'eventaction':(id==='home-fnb')?'fnblink':id,'eventlabel':'','event':'uaevent'});},'panelOpenClick.ccfooter':function(e){dataLayer.push({'eventcategory':'footer'+_division,'eventaction':'chanel.com','event':'uaevent'});}})}};global.__CCFooter={init:init,registerCallbacks:function(callbacks){$.extend(_callbacks,callbacks);},display:display,hide:hide,isDisplayed:function(){return _isDisplayed;},isLatest:function(){return _isLatest;},open:openPanel,close:closePanel,isOpened:function(){return _isOpened;},setClass:function(className){if(_className!==null){$($container).add($footer).removeClass(_className);}
_className=className;$($container).add($footer).addClass(className);},removeClass:function(className){if(_className===className){_className=null;}
$($container).add($footer).removeClass(className);},setItemActive:function(id){$('#'+id).addClass("active");},setItemNormal:function(id){$('#'+id).removeClass("active");},switchActiveItem:function(id){$($footer).find('a').removeClass('active');$('#'+id).addClass("active");},getParams:function(){return _params;}};$(document).trigger("loaded.ccfooter");})(jQuery,window);