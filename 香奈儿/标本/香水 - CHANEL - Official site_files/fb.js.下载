function _attachEvent(){$('.'+_prefix+'-popin').on('click',function(e){e.stopPropagation();});$('.'+_prefix+'-js-close').on('click',function(e){e.preventDefault();e.stopPropagation();_actionController('close',$(this).attr('data-closeTarget'));});$('.'+_prefix+'-js-close').on('touchmove',function(e){if($(this).attr('data-type')!=='languages'){e.preventDefault();e.stopImmediatePropagation();}});$('.'+_prefix+'-js-close').on('touchstart',function(e){(!_isIframe)?window.focus():null;});var stuff={};$(document).on('touchstart','.ccservices-select-list',stuff,function(e){e.data.max=this.scrollHeight-this.offsetHeight;e.data.y=e.originalEvent.pageY;}).on('touchmove','.ccservices-select-list',stuff,function(e){var dy=e.data.y-e.originalEvent.pageY;if((dy<0&&this.scrollTop<1)||(dy>0&&this.scrollTop>=e.data.max)){e.preventDefault();}});if(typeof __CCTracking!='undefined'){$('.'+_prefix+'-popin').on('click','[data-track]',function(){_trackWT.track($(this).data('track'));});}}
function _interceptMessage(){$(window).on('message.ccservices',_communicator);if(_options.useHashChange){if(_support.hashChange){$(window).on('hashchange.ccservices',_router);}else{$('a[href^="#"]').on('click',function(){setTimeout(function(){if(_currentHash!==window.location.hash){_router();}},200);});}}}
function _communicator(msgEvent){if(msgEvent.originalEvent.origin.indexOf(_options.domains)===-1&&((typeof __CCTracking!='undefined')&&_trackWT.config.environnement!=='local')){}
var msgData;try{msgData=JSON.parse(msgEvent.originalEvent.data);_origin=msgData.origin||_defaultOrigin;switch(msgData.action){case"open":case"close":case"close_all":case"success":_setMessage(msgData.message);_actionController(msgData.action,msgData.hash);break;case"show_close_button":case"hide_close_button":_actionController(msgData.action,msgData.hash,msgData.internal);break;case"track":break;default:}}catch(e){}}
function _actionController(action,serviceHash,isInternal){var tabHash=[''];if(serviceHash)tabHash=serviceHash.split('/');if(typeof __CCTracking!='undefined'){_trackWT.track(__ccHelper.formatTag(_tagPlan['manager'],{'%action%':action,'%service%':tabHash[0],'%origin%':_origin}));}
_origin=_defaultOrigin;if(_options.useHashChange&&_usePopin&&!isInternal){var newHash="";switch(action){case"open":newHash=serviceHash;break;case"close":_closeService(serviceHash);newHash=(_historyStack.length>0)?_historyStack[_historyStack.length-1]:"";if(newHash.split('storelocator')[0]==='')newHash="";break;case"close_all":newHash="";break;case"success":_reloadPopin(serviceHash);newHash=(_historyStack.length>0)?_historyStack[_historyStack.length-1]:"";break;case"accessibility":break;default:}
_forceHashChange=true;window.location.hash=newHash;}else{switch(action){case"open":_openService(tabHash[0]);break;case"close":_closeService(tabHash[0]);break;case"close_all":_closeAllServices();break;case"success":_reloadPopin(serviceHash);break;case"show_close_button":case"hide_close_button":_toggleCloseButton(action,serviceHash);break;case"accessibility":break;default:}}}
function _router(){_currentHash=window.location.hash;var hashTab=_currentHash.substr(1).split('/');var serviceHash=hashTab[0];var action="";if(_multilingualService.support&&_multilingualService.unavailable.indexOf(serviceHash)>-1)return;if(serviceHash!==""){action="open";hashTab.shift();if(hashTab.length){_setMessage(hashTab.join('/'));}
_openService(serviceHash);}else{action="close_all";_closeAllServices();}
if(!_forceHashChange&&typeof __CCTracking!='undefined'){_trackWT.track(__ccHelper.formatTag(_tagPlan['manager'],{'%action%':action,'%service%':serviceHash,'%origin%':_origin}));}
_forceHashChange=false;}
function _formatMessage(serviceHash,message){var formatedMsg=$(message.split('/')).filter(function(){return this;}).toArray();if(_routes[serviceHash]!==undefined)formatedMsg.unshift(serviceHash);return formatedMsg.join('/');}
function _sendMessage(serviceHash,message){var idPopin=_routes[serviceHash]||serviceHash;var $popin=$('#'+_prefix+'-'+idPopin);if(idPopin.indexOf('storelocator')>-1){var finalMsg=_formatMessage(serviceHash,'origin-'+_options.division+'/'+message);}else{var finalMsg=_formatMessage(serviceHash,message+'/origin-'+_options.division);}
var domain=(!global.location.origin)?global.location.protocol+"//"+global.location.host:global.location.origin;_msgToSend="";if(!$popin.length)return;switch($popin.attr('data-type')){case"iframe":var $iframe=$popin.find('iframe');var splittedSrc=$iframe.attr('src').split('/');var protocol=(splittedSrc[0]!=="")?splittedSrc[0]:global.location.protocol;domain="*";$iframe.get(0).contentWindow.postMessage(finalMsg,domain);break;case"languages":break;default:}}
function _initPopin(idPopin,callback){var $popin=$('#'+_prefix+'-'+idPopin);if(!$popin.length)return;if($popin.data('loaded')){callback();return;}
if(_currentHash=='#contact'&&$popin.data('loaded')===false&&$popin.attr('form')==='success'){var $iframe=$popin.find('iframe');$iframe.on('load',callback);$iframe.get(0).contentWindow.location.replace($iframe.attr('data-src'));return;}
switch($popin.attr('data-type')){case"iframe":var $iframe=$popin.find('iframe').clone(),dataSrc=$iframe.attr('data-src');if(window.location.hash==='#storelocator/ccc'){dataSrc=dataSrc.replace('storelocator','storelocator/ccc');}
if(_options.lang.length!==0&&_options.newfooter){dataSrc=(dataSrc.indexOf('?')>-1)?dataSrc+'&lang='+_options.lang:dataSrc+'?lang='+_options.lang;}
$popin.find('iframe').remove();$iframe.on('load',callback);$iframe.attr('src',dataSrc);$popin.find('.'+_prefix+'-popin').append($iframe);if(_options.locale=='ar_ME'){$('.ccservices-js-close .ccservices-close').addClass('ar_ME');}
break;case"languages":languagesService.initPopin();break;default:}
$popin.data('loaded',true);}
function _openService(serviceHash){var idPopin=_routes[serviceHash]||serviceHash;if(_popin[idPopin]){var $allPopin=$('.'+_prefix+'-popin-wrapper');var $popin=$('#'+_prefix+'-'+idPopin);var cachedMsg=_msgToSend;var srcPopin=_popin[idPopin].src?_popin[idPopin].src:'';if(!_historyStack.length||_historyStack[_historyStack.length-1]!==serviceHash){_historyStack.push(serviceHash);}
