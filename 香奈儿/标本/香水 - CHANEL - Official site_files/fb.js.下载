break;case"languages":if(_multilingualService.support){var titles=languagesService.getPopinTitles(langValue);tpl.push('<div class="'+_prefix+'-valign">');tpl.push('<div class="'+_prefix+'-popin-header"><div role="heading" aria-level="1" id="'+_prefix+'-languages-title">'+titles.title+'</div><p>'+titles.caption+'</p></div>');tpl.push('<div class="'+_prefix+'-popin-content '+_prefix+'-popin-column-type"></div>');tpl.push('</div><span class="after"></span>');}else{tpl.push('<div class="'+_prefix+'-popin-header"><div role="heading" aria-level="1" id="'+_prefix+'-languages-title">'+(accNavigationLabels?accNavigationLabels.languages.title:'Languages')+'</div></div>');tpl.push('<div class="'+_prefix+'-popin-content '+_prefix+'-popin-column-type"></div>');}
