dth=docWidth,heightList=$skinnedList.outerHeight(),widthList=$skinnedList.outerWidth(),skinnedSelectHeight=$skinnedSelect.outerHeight();if(posTopList+heightList>docHeight){if(posTopSelect>docHeight / 2){maxHeight=Math.min(posTopSelect-10,self.options.maxheight);$skinnedList.css('max-height',maxHeight+'px');heightList=$skinnedList.outerHeight();posTopList=Math.max(offsetTopSelect-heightList,10);}else{maxHeight=Math.min(docHeight-posTopSelect-heightSelect-10,self.options.maxheight);}}
if(self.options.fixwidth||offsetLeftSelect+widthList>docWidth){maxWidth=$skinnedSelect.outerWidth();$skinnedList.find('.'+self.options.prefix+'-option').css('white-space','normal');}
$skinnedList.css({maxHeight:maxHeight+'px',minWidth:$skinnedSelect.outerWidth()+'px',maxWidth:maxWidth+'px',top:self.options.relativeSkinnedLists?skinnedSelectHeight-1+'px':posTopList+'px',left:self.options.relativeSkinnedLists?0:posLeftSelect+'px'}).addClass('opened').data('opened',true);$select.trigger('displayList',[$skinnedList,maxHeight]);self.scrollToSelected();$(document).on('mousedown.bangselect DOMMouseScroll.bangselect mousewheel.bangselect',{obj:self},self.closeSkinnedList).on('keydown.bangselect',{obj:self},self.keyboardManager);}});$skinnedList.delegate('.'+self.options.prefix+'-option',{mouseenter:function(){var $li=$(this);$li.siblings('.active').removeClass('active');$li.addClass('active');},mousedown:function(e){return false;},click:function(){self.selectOption($(this),true,true);}});$select.on('focus.bangselect',function(){$skinnedSelect.addClass('focus');}).on('blur.bangselect focusout.bangselect',function(){$skinnedSelect.removeClass('focus');}).on('keydown.bangselect',{obj:self},self.keyboardManager);$(window).on('resize',{obj:self},self.closeSkinnedList);},scrollToSelected:function(){var self=this,$select=self.$select,$skinnedList=self.$skinnedList,$selectedOption=$skinnedList.find('.selected');$select.trigger('scrollToSelected',[$skinnedList,$selectedOption]);if(self.options.rescroll){if($skinnedList.prop("scrollHeight")<=$skinnedList.height()){$skinnedList.scrollTop(0);}else{var posTopSelected=$selectedOption.position().top,newScrollPos=null;if(posTopSelected<0){newScrollPos=$skinnedList.scrollTop()+posTopSelected;}else if(posTopSelected+$selectedOption.height()>$skinnedList.height()){newScrollPos=$skinnedList.scrollTop()+posTopSelected-$skinnedList.height()+$selectedOption.height();}
if(newScrollPos!=null)$skinnedList.scrollTop(newScrollPos);}}},keyboardManager:function(e){e.stopImmediatePropagation();var self=e.data.obj,keyCode=e.keyCode||e.which,$skinnedSelect=self.$skinnedSelect,$skinnedList=self.$skinnedList,$selectedOption=$skinnedList.find('.selected');switch(keyCode){case 13:case 27:case 32:e.preventDefault();$skinnedSelect.trigger('click');break;case 33:e.preventDefault();var $liToSelect=$($skinnedList.find('.'+self.options.prefix+'-option').get(0));$skinnedList.find('.'+self.options.prefix+'-option').each(function(index){if($(this).position().top>=0){$liToSelect=$(this);return false;}});if(!$liToSelect.is($selectedOption)){self.selectOption($liToSelect,true,false);break;}
break;case 36:e.preventDefault();var $liToSelect=$($skinnedList.find('.'+self.options.prefix+'-option').get(0));self.selectOption($liToSelect,true,false);break;case 34:e.preventDefault();var $liToSelect=$($skinnedList.find('.'+self.options.prefix+'-option').get(-1));$skinnedList.find('.'+self.options.prefix+'-option').each(function(index){if($(this).position().top>=$skinnedList.height())return false;$liToSelect=$(this);});if(!$liToSelect.is($selectedOption)){self.selectOption($liToSelect,true,false);break;}
break;case 35:e.preventDefault();var $liToSelect=$($skinnedList.find('.'+self.options.prefix+'-option').get(-1));self.selectOption($liToSelect,true,false);break;case 37:case 38:e.preventDefault();if($selectedOption.prev().length&&$selectedOption.prev().is('.ccservices-option')){self.selectOption($selectedOption.prev(),true,false);break;}
if($selectedOption.prev().is('.ccservices-optgroup')&&$selectedOption.prev().prev().length){var index=$skinnedList.find('li').index($selectedOption.prev().prev().get(0));self.selectOption($skinnedList.find('li:eq('+index+')'),true,false);}
break;case 39:case 40:e.preventDefault();if($selectedOption.next().length&&$selectedOption.next().is('.ccservices-option')){self.selectOption($selectedOption.next(),true,false);break;}
